<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CodeScannerView">
        <attr name="csvMaskColor" format="color" />
        <attr name="csvVibratorDuration" format="integer" />
        <attr name="csvFrameColor" format="color" />
        <attr name="csvFrameThickness" format="dimension" />
        <attr name="csvFrameThicknessMargin" format="dimension" />
        <attr name="csvFrameCornersSize" format="dimension" />
        <attr name="csvFrameCornersRadius" format="dimension" />
        <attr name="csvFrameAspectRatioWidth" format="float" />
        <attr name="csvFrameAspectRatioHeight" format="float" />
        <attr name="csvFrameSize" format="float" />
        <attr name="csvLazerHeight" format="dimension" />
        <attr name="csvLazerColor" format="color" />
        <attr name="csvLazerVisible" format="boolean" />
        <attr name="csvLazerAnimDuration" format="integer" />

        <attr name="csvFrameMode" format="enum">
            <enum name="oval" value="0" />
            <enum name="rectangle" value="1" />
        </attr>
        <attr name="csvLazerAnim" format="enum">
            <enum name="linear" value="0" />
            <enum name="fade" value="1" />
        </attr>
        <attr name="csvDeniedType" format="enum">
            <enum name="none" value="0" />
            <enum name="snack_bar" value="1" />
            <enum name="dialog" value="2" />
        </attr>
    </declare-styleable>
</resources>